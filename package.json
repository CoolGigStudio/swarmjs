{
  "name": "swarmjs",
  "version": "0.1.0",
  "description": "Agentic framework inspired from OpenAI's swarm framework for TS, JS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/cli/repl.ts",
    "dev:debug": "cross-env DEBUG=true ts-node-dev --respawn --transpile-only src/cli/repl.ts",
    "start": "node dist/cli/repl.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "watch": "tsc -w",
    "generate": "ts-node src/tools/internal/generateSwarm.ts",
    "voice": "ts-node examples/voice-llm/server.ts"
  },
  "keywords": [
    "ai",
    "swarm",
    "openai",
    "chatgpt",
    "agents"
  ],
  "author": "NeuralNets Technologies LLC",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^7.15.0",
    "audio-resampler": "^1.0.1",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "ngrok": "5.0.0-beta.2",
    "node-fetch": "2",
    "openai": "^5.10.2",
    "sox-stream": "^2.0.6",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "twilio": "^5.4.3",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/google-cloud__storage": "^2.3.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.17.14",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
